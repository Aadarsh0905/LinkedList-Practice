package LinkedList.DoublyLInkedList;

class ListNode{
    int data;
    ListNode next;
    ListNode prev;
}
public class ReverseDoublyList {
    public static void main(String[] args) {
        ListNode head=null;
    }
    public static ListNode reverseDLL(ListNode head){
        if(head==null || head.next==null)return head;
        ListNode last=null;
        ListNode curr=head;
        while(curr!=null){
            last=curr.prev;
            curr.prev=curr.next;
            curr.next=last;
            curr=curr.prev;
        }
        if(last!=null){
            head=last.prev;
        }
        return head;
    }
}
