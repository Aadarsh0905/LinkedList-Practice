public static Node CombiningOddAndEvenNode(Node head){
        if(head==null||head.next==null)return head;
        Node odd=head;
        Node even=head.next;
        Node evenHead=head.next;
        while(odd!=null||odd.next!=null){
            odd.next=odd.next.next;
            odd=odd.next.next;
            even.next=even.next.next;
            even=even.next.next;
        }
        odd.next=evenHead;
        return head;
    }
